generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma/client"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(uuid())
  name           String
  email          String   @unique
  passwordHash   String
  imageUrl       String
  createdAt      DateTime @default(now())
}

